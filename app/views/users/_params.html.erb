<%= form_with model: user, local: true do |f| %>

<div class="">
  <%= f.label :user_icon %>
  <%= image_tag user.user_icon %>
</div>
<div class="">
  <%= f.label :user_name %>
  <%= user.user_name %>
</div>

<div class="">
  <%# フォローボタン %>
  <%= render 'relationships/switch', user: @user %>
</div>

<% end %>
